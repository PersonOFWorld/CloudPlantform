<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"     
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cloud.mapper.AddPerMacMapper">
	<!-- 添加个人办公机账号 -->
	<insert id="addAccount" parameterType="counterBean">
		insert into
		personalmachine(personalMachineNum,personalMachinePwd,systemType) values (#{personalMachineNum},#{personalMachinePwd},#{systemType})
	</insert>
</mapper>